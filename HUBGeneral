if (window.location.href.indexOf("https://hubwarez.tv/forum/postador.php?tabid=40") != -1 ) {
    function realizarSolicitacaoComTokenETipo(tipo) {

var codigoFonte = document.documentElement.innerHTML;
var regexToken = /var SECURITYTOKEN = "(.*?)"/;
var match = codigoFonte.match(regexToken);

if (match && match[1]) {
    var token = match[1];

    // Construir a string de consulta (query string)
    var queryString = 'securitytoken=' + encodeURIComponent(token) + '&action=newtituloform&tipo=' + encodeURIComponent(tipo);

    // Opções da requisição AJAX
    var requestOptions = {
        method: 'POST',
        headers: new Headers({
            'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8',
            'X-Requested-With': 'XMLHttpRequest', // Adicione outros cabeçalhos se necessário
            'SecurityToken': token // Adicione o token de segurança ao cabeçalho
        }),
        credentials: 'same-origin', // Manter as credenciais se estiverem no mesmo domínio
        body: queryString
    };

    // URL da solicitação AJAX
    var url = 'https://hubwarez.tv/forum/postador.php?do=novotitulo';

    // Realizar a solicitação AJAX
    fetch(url, requestOptions)
        .then(response => response.text())
        .then(html => {
            // Manipular HTML aqui, se necessário
           // Substituir o conteúdo da página pelo HTML retornado
        document.open();
        document.write(html);
        document.close();
        })
        .catch(error => {
            // Lidar com erros de requisição
            console.error(error.message);
        });
} else {
    console.error('Token de segurança não encontrado.');
}

}
function criarDropdown() {
    // Criar o elemento select
    var tipoDropdown = document.createElement('select');
    tipoDropdown.id = 'tipoDropdown';

    // Opções do dropdown
    var tipos = ['CRIAR TÍTULO','FILME', 'SERIE', 'JOGO', 'FILMEXXX'];
    for (var i = 0; i < tipos.length; i++) {
        var option = document.createElement('option');
        option.value = tipos[i].toLowerCase();
        option.text = tipos[i]; // Capitalizar a primeira letra
        tipoDropdown.appendChild(option);
    }


tipoDropdown.style.borderTop = '0px solid #Cedfeb';
    tipoDropdown.style.borderLeft = '0px solid #Cedfeb';
    tipoDropdown.style.display = 'block';
    tipoDropdown.style.background = '#1C617F url(images/buttons/newbtn_middle.png) repeat-x';
    tipoDropdown.style.minWidth = '150px';
    tipoDropdown.style.width = 'auto !important';
    tipoDropdown.style.width = '150px';
    tipoDropdown.style.textAlign = 'center';
    tipoDropdown.style.color = '#ffffff';
    tipoDropdown.style.font = 'bold 12px Arial, Calibri, Verdana, Geneva, sans-serif';
    tipoDropdown.style.textDecoration = 'none';
    tipoDropdown.style.lineHeight = '40px';
    tipoDropdown.style.padding = '0 10px';
    tipoDropdown.style.height = '40px';
    tipoDropdown.style.lineHeight = '40px';


    // Adicionar o evento de mudança ao dropdown
    tipoDropdown.addEventListener('change', tipoDropdownOnChange);

    // Criar o elemento li para o dropdown
    var dropdownLi = document.createElement('li');
    dropdownLi.appendChild(tipoDropdown);

    // Adicionar o li ao ul com o ID 'navtabs'
    var navtabsUl = document.getElementById('navtabs');
    if (navtabsUl) {
        navtabsUl.appendChild(dropdownLi);
    } else {
        console.error('Elemento com o ID "navtabs" não encontrado.');
    }
}

// Chamar a função para criar e adicionar o menu dropdown
criarDropdown();

// Função para manipular a mudança no menu dropdown
function tipoDropdownOnChange() {

    // Obter o valor selecionado no menu dropdown
    var tipoDropdown = document.getElementById('tipoDropdown');
    var tipoSelecionado = tipoDropdown.options[tipoDropdown.selectedIndex].value;

    // Realizar a solicitação AJAX com base no token e tipo selecionado
    if (tipoSelecionado) {
        realizarSolicitacaoComTokenETipo(tipoSelecionado);
    }
}

// Chamar a função de mudança inicialmente para processar o valor inicial do dropdown
document.getElementById('tipoDropdown').addEventListener('change', tipoDropdownOnChange);


}


