////////////////////////////////////////////////////////////////////////////////////////////////////////////SET DE FILMES
if (window.location.href.indexOf("https://hubwarez.tv/forum/postador.php?do=novotitulo#filme") != -1){

    var codigoFonte = document.documentElement.innerHTML;
var regexToken = /var SECURITYTOKEN = "(.*?)"/;
var match = codigoFonte.match(regexToken);

if (match && match[1]) {
    var token = match[1];

    // Construir a string de consulta (query string)
    var queryString = 'securitytoken=' + encodeURIComponent(token) + '&action=newtituloform&tipo=' + encodeURIComponent('filme');

    // Opções da requisição AJAX
    var requestOptions = {
        method: 'POST',
        headers: new Headers({
            'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8',
            'X-Requested-With': 'XMLHttpRequest', // Adicione outros cabeçalhos se necessário
            'SecurityToken': token // Adicione o token de segurança ao cabeçalho
        }),
        credentials: 'same-origin', // Manter as credenciais se estiverem no mesmo domínio
        body: queryString
    };

    // URL da solicitação AJAX
    var url = 'https://hubwarez.tv/forum/postador.php?do=novotitulo';

    // Realizar a solicitação AJAX
    fetch(url, requestOptions)
        .then(response => response.text())
        .then(html => {
            // Manipular HTML aqui, se necessário
           // Substituir o conteúdo da página pelo HTML retornado
        document.open();
        document.write(html);
        document.close();
        var o_titulo = GM_getValue('o_titulo');
    var titulo = GM_getValue('titulo');
    var genre = GM_getValue('genero');
    var minutos = GM_getValue('minutos');
    var year = GM_getValue('ano');
    var director = GM_getValue('direcao');
    var imdb = GM_getValue('imdb');
    var tmdb = GM_getValue('tmdb');
    var site = GM_getValue('site');
    var actor = GM_getValue('actor');
    var sumario = GM_getValue('sinopse');
    var yt = GM_getValue('yt');
    var exinfo = GM_getValue('exinfo');
    var actor_preview = actor;
    actor_preview = actor_preview.replace(/TABLE\]/gi, "table>");
    actor_preview = actor_preview.replace(/TD\]/gi, "td>");
    actor_preview = actor_preview.replace(/TR\]/gi, "tr>");
    actor_preview = actor_preview.replace(/URL=/gi, 'a href="');
    actor_preview = actor_preview.replace(/\[\/URL]/gi, "</a>");
    actor_preview = actor_preview.replace(/CENTER\]/gi, "center>");
    actor_preview = actor_preview.replace(/B\]/gi, "b>");
    actor_preview = actor_preview.replace(/\[IMG2\]/gi, '<img width="120" src="');
    actor_preview = actor_preview.replace(/\[\/IMG2\]/gi, '"></img>');
    actor_preview = actor_preview.replace(/\[IMG\]/gi, '<img src="');
    actor_preview = actor_preview.replace(/\[\/IMG\]/gi, '"></img>');
    actor_preview = actor_preview.replace(/\[/gi, "<");
    actor_preview = actor_preview.replace(/\]/gi, '">');
    actor_preview = actor_preview.replace(/\n<b>/gi, '<br><b>');
    actor_preview = actor_preview.replace(/"><img/gi, '" target="_blank"><img');
    console.log(actor_preview);

        var intervalId = setInterval(function () {
        if (document.getElementById('movieId')) {
            // Elemento está disponível, podemos atualizar o valor e interromper o intervalo
            document.getElementById('movieId').value = imdb;
            document.getElementById('movieId').dispatchEvent(new Event('change'));
            //document.getElementById('elenco').value = actor;
            clearInterval(intervalId);
        }
    }, 100);
        var intervalId2 = setInterval(function () {
        if (actor != document.getElementById('elenco').value) {
            // Elemento está disponível, podemos atualizar o valor e interromper o intervalo
            document.getElementById('elenco').value = actor;
            document.getElementById('curiosidades').value = exinfo;
            if (document.getElementById('sinopse').value == ''){
            document.getElementById('sinopse').value = sumario;
            }
            if(actor == document.getElementById('elenco').value){
            //document.querySelector('input[value="Enviar novo Titulo"]').click();
                clearInterval(intervalId2);
            }

        }
    }, 1500);
    /*document.getElementById('original').value = o_titulo;
    document.getElementById('traduzido').value = titulo;
    //Genre utiliza JQuery para selecionar mais de um, utilizando array
    try{
        genre = genre.replace(/comedy/g, "commedy");
        genre = genre.replace(/mystery/g, "mistery");
        genre = genre.replace(/horror/g, "terror");
        genre = genre.replace(/music/g, "musical");
        genre = genre.replace(/ /g, ",");

        var genre_array = genre.split(',');
        console.log(genre_array);
    }catch(e){}

    $("#cfield_genre").next().remove();
    $("#cfield_genre").select2({width: "100%"});
    $('#cfield_genre').val(genre_array).trigger("change");
    document.getElementById('cfield_duration').value = minutos;
    document.getElementById('cfield_year').value = year;
    document.getElementById('cfield_direction').value = director;

    document.getElementById('cfield_tmdb').value = tmdb;
    document.getElementById('cfield_site').value = site;
    document.getElementById('cfield_cast').value = actor;
    document.getElementById('cfield_summary').value = sumario;
    document.getElementById('cfield_trailer').value = yt;
    document.getElementById('cfield_curiosity').value = exinfo;*/
    GM_setValue('o_titulo', "");
    GM_setValue('titulo', "");
    GM_setValue('genero', "");
    GM_setValue('minutos', "");
    GM_setValue('ano', "");
    GM_setValue('direcao', "");
    GM_setValue('imdb', "");
    GM_setValue('tmdb', "");
    GM_setValue('site', "");
    GM_setValue('actor', "");
    GM_setValue('sinopse', "");
    GM_setValue('yt', "");
    GM_setValue('exinfo', "");


        })
        .catch(error => {
            // Lidar com erros de requisição
            console.error(error.message);
        });
} else {
    console.error('Token de segurança não encontrado.');
}

